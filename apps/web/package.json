{
  "name": "@piucane/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "node ../../scripts/test-runner.js unit",
    "test:all": "node ../../scripts/test-runner.js",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.config.js tests/integration",
    "test:e2e": "playwright test",
    "test:a11y": "playwright test --config playwright-a11y.config.ts",
    "test:load": "playwright test tests/load",
    "test:lighthouse": "lhci autorun --config lighthouse.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ci": "node ../../scripts/test-runner.js --fail-fast",
    "clean": "rm -rf .next out dist test-results"
  },
  "dependencies": {
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@next/font": "^14.0.3",
    "firebase": "^10.7.0",
    "framer-motion": "^10.16.5",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4",
    "@tanstack/react-query": "^5.8.4",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "sharp": "^0.32.6",
    "stripe": "^14.7.0",
    "@stripe/stripe-js": "^2.1.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@playwright/test": "^1.40.0",
    "@axe-core/playwright": "^4.8.2",
    "axe-playwright": "^1.2.3",
    "jest-axe": "^8.0.0",
    "@lhci/cli": "^0.12.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3"
  }
}