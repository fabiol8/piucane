  "categories": {
    "critical": {
      "description": "CTA che influenzano direttamente la conversione o il revenue",
      "examples": ["onboarding-complete", "checkout-complete", "subscription-start"]
    },
    "high": {
      "description": "CTA importanti per l'engagement e la user experience",
      "examples": ["add-to-cart", "ai-chat-start", "login-start"]
    },
    "medium": {
      "description": "CTA che supportano l'engagement e le funzionalità secondarie",
      "examples": ["product-view", "mission-start", "subscription-manage"]
    },
    "low": {
      "description": "CTA di navigazione e funzionalità accessorie",
      "examples": ["nav-home", "badge-claim", "logout"]
    }
  },
  "validation": {
    "rules": [
      "Ogni CTA deve avere un ID univoco",
      "Ogni CTA deve essere mappato a un evento GA4",
      "I CTA critical e high devono essere testati in E2E",
      "Tutti i CTA devono avere tracking implementato",
      "I CTA devono essere accessibili (WCAG 2.2 AA)"
    ],
    "required_fields": ["id", "description", "location", "component", "ga4Event", "priority"],
    "priority_levels": ["critical", "high", "medium", "low"]
  },
  "implementation": {
    "tracking_attribute": "data-cta-id",
    "ga4_parameter": "cta_id",
    "validation_script": "npm run validate:cta",
    "coverage_requirement": "100%"
  }
}